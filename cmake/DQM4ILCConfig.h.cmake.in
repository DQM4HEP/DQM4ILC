#ifndef DQM4ILCConfig_H
#define DQM4ILCConfig_H

// version macros
#define DQM4ILC_MAJOR_VERSION @DQM4ILC_VERSION_MAJOR@
#define DQM4ILC_MINOR_VERSION @DQM4ILC_VERSION_MINOR@
#define DQM4ILC_PATCH_LEVEL @DQM4ILC_VERSION_PATCH@

#define DQM4ILC_VERSION_STR "@DQM4ILC_VERSION_MAJOR@.@DQM4ILC_VERSION_MINOR@.@DQM4ILC_VERSION_PATCH@"
// for backward compatibility
#define DQM4HEP_VERSION_STR "@DQM4ILC_VERSION_MAJOR@.@DQM4ILC_VERSION_MINOR@.@DQM4ILC_VERSION_PATCH@"

#define DQM4ILC_VERSION_GE( MAJV , MINV , PLEV )  \
     ( (DQM4ILC_MAJOR_VERSION > MAJV) || ( (DQM4ILC_MAJOR_VERSION == MAJV) && (DQM4ILC_MINOR_VERSION > MINV) ) \
  || ( (DQM4ILC_MAJOR_VERSION == MAJV) && (DQM4ILC_MINOR_VERSION == MINV) && (DQM4ILC_PATCH_LEVEL >= PLEV) ) )

// project source dir
#define DQM4ILC_DIR "@CMAKE_INSTALL_PREFIX@"



#endif // DQM4ILCConfig_H
